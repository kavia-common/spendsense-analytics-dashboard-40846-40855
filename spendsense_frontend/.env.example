# Copy to your own environment setup (do NOT commit real secrets).
# CRA requires REACT_APP_ prefix.

# Existing (examples only)
REACT_APP_BACKEND_URL=https://backend.example.com
REACT_APP_API_BASE=https://api.example.com
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=ws://localhost:8000/ws
REACT_APP_NODE_ENV=development
REACT_APP_LOG_LEVEL=info
REACT_APP_PORT=3000
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=analytics_api
REACT_APP_EXPERIMENTS_ENABLED=false

# Optional: analytics-api backend (separate origin)
REACT_APP_ANALYTICS_API_URL=https://analytics-api.example.com
# Optional: override default timeout for analytics calls
REACT_APP_ANALYTICS_API_TIMEOUT_MS=15000
